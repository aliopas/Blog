# Admin Credentials
ADMIN_EMAIL=admin@techinsights.com
ADMIN_PASSWORD=your_secure_password_here

# Database Connection (Aiven Postgres)
DB_HOST=your-postgres-host.aivencloud.com
DB_PORT=28899
DB_NAME=defaultdb
DB_USER=your-db-user
